language set_err(java);

prefix = "RecursiveSet"

:: lexer

eoi:
'a': /a/
'b': /b/
'c': /c/

:: parser

input ::=
	abc xyz def
;

abc ::=
# ERR: @~abc: Cannot resolve set, since it recursively depends on itself.
	  set('a' | ~abc) 'b'
;

xyz ::=
# ERR: @~xyz: Cannot resolve set, since it recursively depends on itself.
# ERR: @'c' & ~xyz: Cannot resolve set, since it recursively depends on itself.
	  set('c' & ~xyz) 'b'
;

def ::=
# ERR: @'c' & def: Cannot resolve set, since it recursively depends on itself.
	  set('c' & def) 'b'
;
