language syntaxNoEoi(java);

prefix = "AA"

:: lexer

identifier {String}: /[a-zA-Z_][a-zA-Z_0-9]*/ -1 { $value = tokenText(); break; }
_skip:          /[\n\t\r ]+/                   	(space)

Lclass: /class/
'{': /\{/
'}': /\}/


:: parser

%input classdef_no_eoi no-eoi;

classdef_no_eoi ::=
	classdef ;

classdef ::=
	Lclass identifier '{' classdeflistopt '}' ;

classdeflist ::=
	classdef
  | classdeflist classdef
;

%%