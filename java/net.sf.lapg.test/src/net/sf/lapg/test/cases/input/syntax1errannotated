
identifier: /@?[a-zA-Z_][A-Za-z_0-9]*/ -1
Licon:      /([1-9][0-9]*|0[0-7]*|0[xX][0-9a-fA-F]+)([uU](l|L|ll|LL)?|(l|L|ll|LL)[uU]?)?/
_skip:      /[\t\r\n ]+/    { continue; }
aaaa: /aaaa/
bbbb: /bbbb/

input ::=
	list ;
	
list ::=
	list_item
	| list list_item
;

[weight:5,name:"wwo"]
list_item ::=
	[name:"rule1"] identifier [ids: [4,2,3]]
	| [name:"rule2"] Licon
;

[name:"aaa", ref:notexistingsym]
tempanno ::=
	aaaa
;

[name:"bbb"]
tempanno ::=
	bbbb
;
