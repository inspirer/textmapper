private static final boolean DEBUG_SYNTAX = true;
byte[] buff = new byte[1025];
int l, end;
InputStream input;

void fillb() {
	l = 0;
	try {
		end = input.read( buff, 0, 1024 );
		if( end == -1 )
			end = 0;
	} catch( IOException ex ) {
		end = 0;
	}
	buff[end] = 0;
}

